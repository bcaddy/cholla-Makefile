{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "PRECISION=1",
                "FLOAT_TYPEDEF=double",
                "EXACT_CUDA_H=1",
                "CUDA"
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/g++",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}